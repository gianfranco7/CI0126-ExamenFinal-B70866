@{
    ViewBag.Title = "ProductList";
}
<!DOCTYPE html>
<html>
<body>
    <h4 class="text-center">
        Productos Disponibles
    </h4>
    @{
        foreach (var product in ViewBag.products)
        {
            <div>
                @product.name
            </div>
            <div>
                @product.description
            </div>
            <div>
                @product.discountAmount
            </div>
            <div>
                @product.name
            </div>
            <div>
                @product.name
            </div>
        }
    }
</body>
</html>